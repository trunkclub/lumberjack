{
  "name": "lumberjack",
  "version": "1.0.0",
  "description": "An automated accessibility crawler.",
  "author": "Beck Kramer <rebecca.kramer@nordstrom.com>",
  "main": "index.ts",
  "scripts": {
    "app": "cd app && yarn start",
    "commitizen": "commitizen",
    "generate-docs": "jsdoc -c ./jsdoc.json",
    "get-report-ids": "node -e 'require(\"./utils/ReportUtils\").getReportIds()'",
    "get-tally": "node -e 'require(\"./utils/ViolationUtils\").getViolationTallyData()'",
    "lint": "eslint 'utils/**/*.ts' --max-warnings=0",
    "lumberjack": "ts-node utils/index.ts",
    "prettier": "prettier --write '{app,utils}/**/*.{ts,js,json,scss,md}'",
    "semantic-release": "semantic-release",
    "server": "node server/index.js",
    "setup": "yarn install && cd client && yarn install",
    "start": "yarn server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "MIT",
  "dependencies": {
    "@axe-core/puppeteer": "^4.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/mkdirp": "^1.0.0",
    "axe-core": "^4.0.2",
    "chalk": "^2.4.2",
    "cli-color": "^2.0.0",
    "dayjs": "^1.8.36",
    "express": "^4.16.4",
    "inquirer": "^7.0.4",
    "mkdirp": "^1.0.4",
    "puppeteer": "^5.4.1",
    "typedoc": "^0.17.4",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/node": "^13.13.0",
    "@types/puppeteer": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "better-docs": "^2.0.1",
    "cz-conventional-changelog": "3.3.0",
    "docdash": "^1.2.0",
    "eslint": "5.12.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^25.4.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc": "^3.6.4",
    "prettier": "1.18.2",
    "semantic-release": "^17.0.4",
    "ts-node": "^8.10.1",
    "yargs": "^13.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
